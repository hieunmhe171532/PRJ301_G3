<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>UserLogin</servlet-name>
        <servlet-class>UserController.UserLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>AdminController.AdminLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCourse</servlet-name>
        <servlet-class>AdminController.AddCourse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditCourse</servlet-name>
        <servlet-class>AdminController.EditCourse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CourseList</servlet-name>
        <servlet-class>UserController.CourseList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailCourse</servlet-name>
        <servlet-class>UserController.DetailCourse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>UserController.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Enroll</servlet-name>
        <servlet-class>UserController.Enroll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnrollmentStatusDAO</servlet-name>
        <servlet-class>DAO.EnrollmentStatusDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllEnrollments</servlet-name>
        <servlet-class>AllEnrollments</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllEnrollment</servlet-name>
        <servlet-class>UserController.AllEnrollment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLogin</servlet-name>
        <url-pattern>/userlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/adminlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCourse</servlet-name>
        <url-pattern>/addcourse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditCourse</servlet-name>
        <url-pattern>/editcourse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CourseList</servlet-name>
        <url-pattern>/courseList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailCourse</servlet-name>
        <url-pattern>/detailCourse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Enroll</servlet-name>
        <url-pattern>/enroll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnrollmentStatusDAO</servlet-name>
        <url-pattern>/enrollmentStatusDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllEnrollment</servlet-name>
        <url-pattern>/AllEnrollment</url-pattern>
    </servlet-mapping>
</web-app>
